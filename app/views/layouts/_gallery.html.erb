<!-- Page Content -->
<div class="container">
  <div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Thumbnail Gallery</h1>
    </div>

    <% object.each do |image| %>
      <% if image.present? %>
        <div class="col-lg-3 col-md-4 col-xs-6 thumb">
          <a class="thumbnail" href="#">
            <%= image_tag(image.file.url(:medium)) %>
          </a>
        </div>
      <% end %>  
    <% end %>
  </div>
</div>
