<!-- Page Content -->
<div class="container">
  <div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Gallery</h1>
    </div>

    <% object.each do |image| %>
      <% if image.present? %>
        <div class="col-lg-3 col-md-4 col-xs-6 thumb">
          <%= link_to image_tag(image.file.url(:medium)), image_path(image), class: "thumbnail" %>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
